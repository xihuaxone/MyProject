docker build -f ./dockerfile -t my_project:{{version}} .

sudo docker run -p 8080:8080 --net=host --name=my_project -v /usr/local/my_project/logs/:/MyProject/my_project/logs/ -d {{image id}}

docker login --username=xihuaxxone registry.cn-hangzhou.aliyuncs.com

cat ~/.docker/config.json

docker tag {{ImageId}} registry.cn-hangzhou.aliyuncs.com/xihua/my_project:{{image version}}

docker push registry.cn-hangzhou.aliyuncs.com/xihua/my_project:{{image version}}

docker pull registry.cn-hangzhou.aliyuncs.com/xihua/my_project:{{image version}}


